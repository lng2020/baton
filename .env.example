# Anthropic API Key (required)
ANTHROPIC_API_KEY=sk-ant-xxxxx

# Database password (required if using PostgreSQL)
DB_PASSWORD=your_secure_password

# Web Manager authentication token (recommended)
WEB_AUTH_TOKEN=your_auth_token

# Voice recognition API Key (optional)
VOICE_API_KEY=your_voice_api_key

# Enable database backup (optional)
DB_BACKUP_ENABLED=

# Backup retention count, default 24 (optional)
BACKUP_RETAIN_COUNT=24
